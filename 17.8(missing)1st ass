#include <stdio.h>

int main()
{
    int arr[]={1,2,3,4,5,6,8,9,10};
    int len =sizeof (arr) /sizeof (int);
    int i;
    for(i=0;arr[i]<len;i++)
    {   
        
        int k=arr[i];
        if(k+1!=arr[i+1])
        {
            printf("%d is missing",k+1);
        }
            
    }
    return 0;
}
