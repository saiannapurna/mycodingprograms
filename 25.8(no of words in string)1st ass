#include <stdio.h>
#include<string.h>
int main()
{
    char str[100];
    int i,count=0;
    scanf("%[^\n]s",str);
            for(i=0;str[i]!='\0';i++)
            {
             if( str[i]==' ' && str[i+1]!=' ')
                count=count+1;
            }

       printf("%d ",count+1);
        return 0;
}
